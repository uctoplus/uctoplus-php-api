{
    "name": "uctoplus/uctoplus-php-api",
    "description": "Production environment is located at `https://api.moje.uctoplus.sk/production/`.  Sandbox environment is located at `https://api.moje.uctoplus.sk/sandbox/`.  All communication with API is encoded in UTF-8. This REST API is based on Open API v3 standard. For help with implementation or access to the test environment please contact our helpdesk",
    "keywords": [
        "uctoplus.sk",
        "\u00da\u010dto+",
        "sdk",
        "php",
        "uctoplus",
        "rest",
        "api"
    ],
    "homepage": "https://uctoplus.sk",
    "license": "MIT",
    "authors": [
        {
            "name": "\u00da\u010dto+",
            "homepage": "https://uctoplus.sk"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Uctoplus\\API\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uctoplus\\API\\Test\\": "test/"
        }
    },
    "version": "3.1.2"
}